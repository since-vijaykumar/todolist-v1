 <%- include("header")-%>

  <div class="box" id="heading">
    <h1> <%= listTitle %></h1>
  </div>

  <div class="box">
    <% for( var x of newItem) { %>
    <div class="item">
      <input type="checkbox">
      <p> <%= x %> </p>
    </div>
    <%} %>
  </div>

  <div class="box">
    <form action="/" method="post">
      <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
      <button type="submit" name="list" value='<%= listTitle %>'>+</button>
    </form>
  </div>

<%- include("footer") -%>
